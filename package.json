{
  "name": "btra",
  "version": "0.0.5",
  "description": "BodyTrace Request/Response Time Analizer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndaidong/btra.git"
  },
  "homepage": "https://www.npmjs.com/package/btra",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "nsp": "nsp check --output checkstyle",
    "report": "nyc report --reporter=lcov",
    "tape": "NODE_ENV=test nyc tape test/start.js | tap-spec",
    "pretest": "npm run lint && npm run nsp",
    "test": "npm run tape",
    "posttest": "npm run report",
    "coveralls": "npm test && cat ./coverage/lcov.info | coveralls",
    "start": "node index -h"
  },
  "bin": {
    "btra": "./index.js"
  },
  "author": "@ndaidong",
  "dependencies": {
    "axios": "0.x.x",
    "bellajs": "6.x.x",
    "chalk": "1.x.x",
    "cli-table2": "0.x.x",
    "commander": "2.x.x",
    "mkdirp": "0.x.x",
    "pretty-ms": "2.x.x",
    "promise.prototype.finally": "2.x.x"
  },
  "devDependencies": {
    "coveralls": "2.x.x",
    "eslint": "3.x.x",
    "eslint-config-ggc": "1.x.x",
    "nsp": "2.x.x",
    "nyc": "10.x.x",
    "tap-spec": "4.x.x",
    "tape": "4.x.x"
  },
  "license": "MIT"
}
